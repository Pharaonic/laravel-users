{
    "name": "pharaonic/laravel-users",
    "description": "Laravel Users (Roles & Permissions, Devices, Password: Auto Hashing & History, Actions: Likes, Rates, Subscribes, Votes, Follows, Favourties, Bookmarks).",
    "keywords": [
        "laravel roles",
        "laravel permissions",
        "laravel user devices",
        "laravel user passwords",
        "laravel user passwords hashed",
        "laravel user passwords history",
        "laravel likes",
        "laravel rates",
        "laravel subscribes",
        "laravel subscribers",
        "laravel votes",
        "laravel follows",
        "laravel followers",
        "laravel favourties",
        "laravel bookmarks",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggi@raggitech.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0",
        "pharaonic/laravel-agents-detector": ">=1.0.3",
        "pharaonic/laravel-translatable": ">=1.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Users\\UsersServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Users\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}